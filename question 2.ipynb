    "def square_even_numbers(input_list):\n",
    "    return [x**2 for x in input_list if x % 2 == 0]\n",
    "\n",
    "# Example usage:\n",
    "numbers = [1, 2, 3, 4, 5, 6]\n",
    "squared_evens = square_even_numbers(numbers)\n",
    "print(squared_evens)  # Output: [4, 16, 36]\n",
    "\n"
   ]
